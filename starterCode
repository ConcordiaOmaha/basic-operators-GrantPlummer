
// Example program
#include <iostream>
#include <string>

int main()
{

  int num1 = 0; 
  int num2 = 0; 
  std::cout << "enter a number: "; 
  std::cin >> num1; 
  std::cout << "\nenter a second number: ";
  std::cin >> num2;
  
  int output = num1 * num2;
  
  std::cout << "Multiplying your numbers gives " << output;
  
}
